{
  "name": "qgl-github",
  "version": "0.0.1",
  "description": "Tool to talk to github via GraphQL",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "tsc-watch": "./node_modules/typescript/bin/tsc -w",
    "build-dashboard":
      "webpack-dashboard -- node ./node_modules/webpack/bin/webpack.js --env.build --mode=production",
    "build":
      "./node_modules/webpack/bin/webpack.js --env.build --mode=production",
    "start":
      "webpack-dashboard -- node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js -w --host=0.0.0.0 --mode=development",
    "start-dev-server":
      "./node_modules/webpack-dev-server/bin/webpack-dev-server.js -w --host=0.0.0.0 --mode=development"
  },
  "jest": {
    "moduleFileExtensions": ["ts", "tsx", "js"],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "setupFiles": ["<rootDir>/config/shim.js", "jest-localstorage-mock"],
    "setupTestFrameworkScriptFile": "<rootDir>/config/preprocessor_test.js",
    "testMatch": ["**/__tests__/**_test.(ts|tsx|js)"],
    "collectCoverageFrom": ["src/**/*.{ts,js,tsx}"],
    "testURL": "http://test.com/unknown",
    "testEnvironment": "jest-environment-jsdom-global"
  },
  "author": "Christof Sirk",
  "license": "MIT",
  "devDependencies": {
    "@types/classnames": "^2.2.6",
    "@types/enzyme": "^3.1.14",
    "@types/jest": "^23.3.3",
    "@types/node": "^8.10.34",
    "@types/node-uuid": "^0.0.28",
    "@types/plotly.js": "^1.38.8",
    "@types/qs": "^6.5.1",
    "@types/ramda": "^0.25.39",
    "@types/react": "^16.4.15",
    "@types/react-dom": "^16.0.8",
    "@types/react-router-dom": "^4.3.1",
    "@types/request": "^2.47.1",
    "@types/request-promise-native": "^1.0.15",
    "@types/webpack": "^4.4.14",
    "awesome-typescript-loader": "^5.2.1",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "graphql-tools": "^3.1.1",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.6.0",
    "jest-environment-jsdom": "^23.4.0",
    "jest-environment-jsdom-global": "^1.1.0",
    "jest-localstorage-mock": "^2.2.0",
    "react-test-renderer": "^16.5.1",
    "source-map-loader": "^0.2.4",
    "ts-jest": "^23.1.4",
    "tslint": "^5.11.0",
    "typescript": "^3.1.0",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.0",
    "webpack-dashboard": "^2.0.0",
    "webpack-dev-server": "^3.1.8"
  },
  "dependencies": {
    "@material-ui/core": "^3.0.3",
    "@material-ui/icons": "^3.0.1",
    "apollo-cache-inmemory": "^1.2.10",
    "apollo-cache-persist": "^0.1.1",
    "apollo-client": "^2.4.2",
    "apollo-link": "^1.2.3",
    "apollo-link-context": "^1.0.9",
    "apollo-link-http": "^1.5.5",
    "classnames": "^2.2.6",
    "commander": "^2.18.0",
    "express": "^4.16.3",
    "graphql": "^14.0.2",
    "graphql-tag": "^2.9.2",
    "netlify-auth-providers": "^1.0.0-alpha5",
    "node-uuid": "^1.4.8",
    "plotly.js": "^1.41.0",
    "qs": "^6.5.2",
    "ramda": "^0.25.0",
    "react": "^16.5.1",
    "react-dom": "^16.5.1",
    "react-markdown": "^3.6.0",
    "react-plotlyjs-ts": "^2.1.0",
    "react-router-dom": "^4.3.1",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.5"
  }
}
