{
  "name": "qgl-github",
  "version": "0.0.1",
  "description": "Tool to talk to github via GraphQL",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "tsc-watch": "./node_modules/typescript/bin/tsc -w",
    "build-dashboard":
      "webpack-dashboard -- node ./node_modules/webpack/bin/webpack.js --env.build --mode=production",
    "build":
      "./node_modules/webpack/bin/webpack.js --env.build --mode=production",
    "start":
      "webpack-dashboard -- node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js -w --host=0.0.0.0 --mode=development",
    "start-dev-server":
      "./node_modules/webpack-dev-server/bin/webpack-dev-server.js -w --host=0.0.0.0 --mode=development"
  },
  "jest": {
    "moduleFileExtensions": ["ts", "tsx", "js"],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "setupFiles": ["<rootDir>/config/shim.js", "jest-localstorage-mock"],
    "setupFilesAfterEnv": ["<rootDir>/config/preprocessor_test.js"],
    "testMatch": ["**/__tests__/**/*_test.(ts|tsx|js)"],
    "collectCoverageFrom": ["src/**/*.{ts,js,tsx}"],
    "testURL": "http://test.com/unknown",
    "testEnvironment": "jest-environment-jsdom-global"
  },
  "author": "Christof Sirk",
  "license": "MIT",
  "devDependencies": {
    "@types/classnames": "^2.2.9",
    "@types/enzyme": "^3.10.3",
    "@types/jest": "^24.0.18",
    "@types/node": "^8.10.54",
    "@types/plotly.js": "^1.44.11",
    "@types/qs": "^6.5.3",
    "@types/ramda": "^0.26.26",
    "@types/react": "^16.9.5",
    "@types/react-dom": "^16.9.1",
    "@types/react-router-dom": "^5.1.0",
    "@types/request": "^2.48.3",
    "@types/request-promise-native": "^1.0.17",
    "@types/uuid": "^3.4.5",
    "@types/webpack": "^4.39.2",
    "awesome-typescript-loader": "^5.2.1",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "graphql-tools": "^4.0.5",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.9.0",
    "jest-environment-jsdom": "^24.9.0",
    "jest-environment-jsdom-global": "^1.2.0",
    "jest-localstorage-mock": "^2.4.0",
    "react-test-renderer": "^16.10.2",
    "source-map-loader": "^0.2.4",
    "ts-jest": "^24.1.0",
    "tslint": "^5.20.0",
    "typescript": "^3.6.3",
    "webpack": "^4.41.0",
    "webpack-cli": "^3.3.9",
    "webpack-dashboard": "^3.2.0",
    "webpack-dev-server": "^3.8.2"
  },
  "dependencies": {
    "@material-ui/core": "^4.5.0",
    "@material-ui/icons": "^4.4.3",
    "@octokit/rest": "^16.31.0",
    "apollo-cache-inmemory": "^1.6.3",
    "apollo-cache-persist": "^0.1.1",
    "apollo-client": "^2.6.4",
    "apollo-link": "^1.2.13",
    "apollo-link-context": "^1.0.19",
    "apollo-link-http": "^1.5.16",
    "classnames": "^2.2.6",
    "commander": "^3.0.2",
    "express": "^4.17.1",
    "github-rebase": "^1.1.0",
    "graphql": "^14.5.8",
    "graphql-tag": "^2.10.1",
    "netlify-auth-providers": "^1.0.0-alpha5",
    "plotly.js": "^1.49.5",
    "qs": "^6.9.0",
    "ramda": "^0.26.1",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-markdown": "^4.2.2",
    "react-plotlyjs-ts": "^2.1.0",
    "react-router-dom": "^5.1.2",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.7",
    "uuid": "^3.3.3"
  }
}
