{
  "name": "qgl-github",
  "version": "0.0.1",
  "description": "Tool to talk to github via GraphQL",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "tsc-watch": "./node_modules/typescript/bin/tsc -w",
    "build-dashboard":
      "webpack-dashboard -- node ./node_modules/webpack/bin/webpack.js --env.build --mode=production",
    "build":
      "./node_modules/webpack/bin/webpack.js --env.build --mode=production",
    "start":
      "webpack-dashboard -- node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js -w --host=0.0.0.0 --mode=development",
    "start-dev-server":
      "./node_modules/webpack-dev-server/bin/webpack-dev-server.js -w --host=0.0.0.0 --mode=development"
  },
  "jest": {
    "moduleFileExtensions": ["ts", "tsx", "js"],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "setupFiles": ["<rootDir>/config/shim.js", "jest-localstorage-mock"],
    "setupFilesAfterEnv": ["<rootDir>/config/preprocessor_test.js"],
    "testMatch": ["**/__tests__/**/*_test.(ts|tsx|js)"],
    "collectCoverageFrom": ["src/**/*.{ts,js,tsx}"],
    "testURL": "http://test.com/unknown",
    "testEnvironment": "jest-environment-jsdom-global"
  },
  "author": "Christof Sirk",
  "license": "MIT",
  "devDependencies": {
    "@types/classnames": "^2.2.8",
    "@types/enzyme": "^3.9.2",
    "@types/jest": "^24.0.13",
    "@types/node": "^8.10.48",
    "@types/plotly.js": "^1.44.6",
    "@types/qs": "^6.5.3",
    "@types/ramda": "^0.26.8",
    "@types/react": "^16.8.18",
    "@types/react-dom": "^16.8.4",
    "@types/react-router-dom": "^4.3.3",
    "@types/request": "^2.48.1",
    "@types/request-promise-native": "^1.0.16",
    "@types/uuid": "^3.4.4",
    "@types/webpack": "^4.4.32",
    "awesome-typescript-loader": "^5.2.1",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.13.2",
    "graphql-tools": "^4.0.4",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.8.0",
    "jest-environment-jsdom": "^24.8.0",
    "jest-environment-jsdom-global": "^1.2.0",
    "jest-localstorage-mock": "^2.4.0",
    "react-test-renderer": "^16.8.6",
    "source-map-loader": "^0.2.4",
    "ts-jest": "^24.0.2",
    "tslint": "^5.16.0",
    "typescript": "^3.4.5",
    "webpack": "^4.32.2",
    "webpack-cli": "^3.3.2",
    "webpack-dashboard": "^3.0.7",
    "webpack-dev-server": "^3.4.1"
  },
  "dependencies": {
    "@material-ui/core": "^4.0.1",
    "@material-ui/icons": "^4.0.1",
    "apollo-cache-inmemory": "^1.6.0",
    "apollo-cache-persist": "^0.1.1",
    "apollo-client": "^2.6.0",
    "apollo-link": "^1.2.11",
    "apollo-link-context": "^1.0.17",
    "apollo-link-http": "^1.5.14",
    "classnames": "^2.2.6",
    "commander": "^2.20.0",
    "express": "^4.17.0",
    "graphql": "^14.3.0",
    "graphql-tag": "^2.10.1",
    "netlify-auth-providers": "^1.0.0-alpha5",
    "plotly.js": "^1.47.4",
    "qs": "^6.7.0",
    "ramda": "^0.26.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-markdown": "^4.0.8",
    "react-plotlyjs-ts": "^2.1.0",
    "react-router-dom": "^5.0.0",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.7",
    "uuid": "^3.3.2"
  }
}
