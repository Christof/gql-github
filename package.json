{
  "name": "qgl-github",
  "version": "0.0.1",
  "description": "Tool to talk to github via GraphQL",
  "main": "index.js",
  "scripts": {
    "test": "jest --coverage",
    "tsc-watch": "./node_modules/typescript/bin/tsc -w",
    "webpack": "./node_modules/webpack/bin/webpack.js",
    "start":
      "./node_modules/webpack-dev-server/bin/webpack-dev-server.js -w --host=0.0.0.0"
  },
  "jest": {
    "moduleFileExtensions": ["ts", "tsx", "js"],
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/config/preprocessor.js"
    },
    "setupFiles": ["<rootDir>/config/shim.js"],
    "setupTestFrameworkScriptFile": "<rootDir>/config/preprocessor_test.js",
    "testMatch": ["**/__tests__/**.(ts|tsx|js)"],
    "collectCoverageFrom": ["src/**/*.{ts,js,tsx}"],
    "testURL": "http://test.com/unknown"
  },
  "author": "Christof Sirk",
  "license": "MIT",
  "devDependencies": {
    "@types/commander": "^2.11.0",
    "@types/enzyme": "^3.1.8",
    "@types/jest": "^22.0.1",
    "@types/node": "^8.5.2",
    "@types/node-uuid": "^0.0.28",
    "@types/plotly.js": "^1.28.10",
    "@types/qs": "^6.5.1",
    "@types/react": "^16.0.31",
    "@types/react-dom": "^16.0.3",
    "@types/react-router-dom": "^4.2.3",
    "@types/request": "^2.0.8",
    "@types/request-promise-native": "^1.0.10",
    "awesome-typescript-loader": "^3.4.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "graphql-tools": "^2.19.0",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^22.1.1",
    "react-test-renderer": "^16.2.0",
    "source-map-loader": "^0.2.3",
    "tslint": "^5.8.0",
    "typescript": "^2.6.1",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.9.7"
  },
  "dependencies": {
    "apollo-client-preset": "^1.0.3",
    "apollo-link": "^1.0.3",
    "apollo-link-context": "^1.0.1",
    "commander": "^2.12.1",
    "express": "^4.16.2",
    "graphql": "^0.11.7",
    "graphql-tag": "^2.5.0",
    "material-ui": "^1.0.0-beta.29",
    "netlify-auth-providers": "^1.0.0-alpha5",
    "node-uuid": "^1.4.8",
    "plotly.js": "^1.31.2",
    "qs": "^6.5.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-markdown": "^3.1.4",
    "react-plotlyjs-ts": "^1.1.1",
    "react-router-dom": "^4.2.2",
    "request": "^2.83.0",
    "request-promise-native": "^1.0.5"
  }
}
