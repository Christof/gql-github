{
  "name": "qgl-github",
  "version": "0.0.1",
  "description": "Tool to talk to github via GraphQL",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "tsc-watch": "./node_modules/typescript/bin/tsc -w",
    "build-dashboard":
      "webpack-dashboard -- node ./node_modules/webpack/bin/webpack.js --env.build --mode=production",
    "build":
      "./node_modules/webpack/bin/webpack.js --env.build --mode=production",
    "start":
      "webpack-dashboard -- node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js -w --host=0.0.0.0 --mode=development",
    "start-dev-server":
      "./node_modules/webpack-dev-server/bin/webpack-dev-server.js -w --host=0.0.0.0 --mode=development"
  },
  "jest": {
    "moduleFileExtensions": ["ts", "tsx", "js"],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "setupFiles": ["<rootDir>/config/shim.js", "jest-localstorage-mock"],
    "setupTestFrameworkScriptFile": "<rootDir>/config/preprocessor_test.js",
    "testMatch": ["**/__tests__/**_test.(ts|tsx|js)"],
    "collectCoverageFrom": ["src/**/*.{ts,js,tsx}"],
    "testURL": "http://test.com/unknown",
    "testEnvironment": "jest-environment-jsdom-global",
    "mapCoverage": true
  },
  "author": "Christof Sirk",
  "license": "MIT",
  "devDependencies": {
    "@types/commander": "^2.11.0",
    "@types/enzyme": "^3.1.8",
    "@types/jest": "^22.0.1",
    "@types/node": "^8.10.8",
    "@types/node-uuid": "^0.0.28",
    "@types/plotly.js": "^1.28.10",
    "@types/qs": "^6.5.1",
    "@types/react": "^16.3.10",
    "@types/react-dom": "^16.0.5",
    "@types/react-router-dom": "^4.2.6",
    "@types/request": "^2.0.8",
    "@types/request-promise-native": "^1.0.14",
    "@types/webpack": "^4.1.3",
    "awesome-typescript-loader": "^5.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "graphql-tools": "^2.24.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^22.1.1",
    "jest-environment-jsdom": "^22.4.0",
    "jest-environment-jsdom-global": "^1.0.3",
    "jest-localstorage-mock": "^2.2.0",
    "react-test-renderer": "^16.3.1",
    "source-map-loader": "^0.2.3",
    "ts-jest": "^22.0.3",
    "tslint": "^5.8.0",
    "typescript": "^2.8.1",
    "webpack": "^4.5.0",
    "webpack-cli": "^2.0.14",
    "webpack-dashboard": "^1.1.1",
    "webpack-dev-server": "^3.1.3"
  },
  "dependencies": {
    "apollo-cache-inmemory": "^1.1.12",
    "apollo-client": "^2.2.8",
    "apollo-link": "^1.2.2",
    "apollo-link-context": "^1.0.8",
    "apollo-link-http": "^1.5.4",
    "commander": "^2.15.1",
    "express": "^4.16.3",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.8.0",
    "material-ui": "^1.0.0-beta.41",
    "netlify-auth-providers": "^1.0.0-alpha5",
    "node-uuid": "^1.4.8",
    "plotly.js": "^1.31.2",
    "qs": "^6.5.1",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-markdown": "^3.3.0",
    "react-plotlyjs-ts": "^1.1.1",
    "react-router-dom": "^4.2.2",
    "request": "^2.85.0",
    "request-promise-native": "^1.0.5"
  }
}
