{
  "name": "qgl-github",
  "version": "0.0.1",
  "description": "Tool to talk to github via GraphQL",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "tsc-watch": "./node_modules/typescript/bin/tsc -w",
    "build-dashboard":
      "webpack-dashboard -- node ./node_modules/webpack/bin/webpack.js --env.build --mode=production",
    "build":
      "./node_modules/webpack/bin/webpack.js --env.build --mode=production",
    "start":
      "webpack-dashboard -- node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js -w --host=0.0.0.0 --mode=development",
    "start-dev-server":
      "./node_modules/webpack-dev-server/bin/webpack-dev-server.js -w --host=0.0.0.0 --mode=development"
  },
  "jest": {
    "moduleFileExtensions": ["ts", "tsx", "js"],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "setupFiles": ["<rootDir>/config/shim.js", "jest-localstorage-mock"],
    "setupTestFrameworkScriptFile": "<rootDir>/config/preprocessor_test.js",
    "testMatch": ["**/__tests__/**_test.(ts|tsx|js)"],
    "collectCoverageFrom": ["src/**/*.{ts,js,tsx}"],
    "testURL": "http://test.com/unknown",
    "testEnvironment": "jest-environment-jsdom-global"
  },
  "author": "Christof Sirk",
  "license": "MIT",
  "devDependencies": {
    "@types/classnames": "^2.2.6",
    "@types/enzyme": "^3.1.15",
    "@types/jest": "^23.3.11",
    "@types/node": "^8.10.39",
    "@types/plotly.js": "^1.42.0",
    "@types/qs": "^6.5.1",
    "@types/ramda": "^0.26.0",
    "@types/react": "^16.7.18",
    "@types/react-dom": "^16.0.11",
    "@types/react-router-dom": "^4.3.1",
    "@types/request": "^2.48.1",
    "@types/request-promise-native": "^1.0.15",
    "@types/uuid": "^3.4.4",
    "@types/webpack": "^4.4.22",
    "awesome-typescript-loader": "^5.2.1",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "graphql-tools": "^4.0.3",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.6.0",
    "jest-environment-jsdom": "^23.4.0",
    "jest-environment-jsdom-global": "^1.1.0",
    "jest-localstorage-mock": "^2.3.0",
    "react-test-renderer": "^16.7.0",
    "source-map-loader": "^0.2.4",
    "ts-jest": "^23.10.5",
    "tslint": "^5.12.0",
    "typescript": "^3.2.2",
    "webpack": "^4.28.3",
    "webpack-cli": "^3.1.2",
    "webpack-dashboard": "^2.0.0",
    "webpack-dev-server": "^3.1.14"
  },
  "dependencies": {
    "@material-ui/core": "^3.8.1",
    "@material-ui/icons": "^3.0.1",
    "apollo-cache-inmemory": "^1.3.12",
    "apollo-cache-persist": "^0.1.1",
    "apollo-client": "^2.4.8",
    "apollo-link": "^1.2.6",
    "apollo-link-context": "^1.0.12",
    "apollo-link-http": "^1.5.9",
    "classnames": "^2.2.6",
    "commander": "^2.19.0",
    "express": "^4.16.4",
    "graphql": "^14.0.2",
    "graphql-tag": "^2.10.0",
    "netlify-auth-providers": "^1.0.0-alpha5",
    "plotly.js": "^1.43.1",
    "qs": "^6.6.0",
    "ramda": "^0.26.1",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-markdown": "^4.0.4",
    "react-plotlyjs-ts": "^2.1.0",
    "react-router-dom": "^4.3.1",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.5",
    "uuid": "^3.3.2"
  }
}
